TYPE NMHDR
	XLONG	.hwndFrom 
	XLONG	.idFrom 
	XLONG	.code 
END TYPE

TYPE INITCOMMONCONTROLSEX
	ULONG	.dwSize  ' size of this structure
	ULONG	.dwICC   ' flags indicating which classes to be initialized
END TYPE

TYPE COLORSCHEME
	ULONG	.dwSize
	ULONG	.clrBtnHighlight		' highlight color
	ULONG	.clrBtnShadow    		' shadow color
END TYPE

TYPE NMTOOLTIPSCREATED
	NMHDR	.hdr  
	ULONG	.hwndToolTips
END TYPE

TYPE COMCTL_POINT   ' use POINT
	XLONG	.x
	XLONG	.y
END TYPE

TYPE NMMOUSE
	NMHDR	.hdr
	ULONG	.dwItemSpec
	ULONG	.dwItemData
	POINT	.pt
	ULONG	.dwHitInfo  	' any specifics about where on the item or control the mouse is
END TYPE

TYPE NMCLICK
	NMHDR	.hdr 
	ULONG	.dwItemSpec
	ULONG	.dwItemData
	POINT	.pt 
	ULONG	.dwHitInfo  	' any specifics about where on the item or control the mouse is
END TYPE

' Generic structure to request an object of a specific type.

TYPE NMOBJECTNOTIFY
	NMHDR	.hdr
	ULONG	.iItem
	ULONG	.piid
	ULONG	.pObject
	ULONG	.hResult
	ULONG	.dwFlags
END TYPE

' Generic structure for a key

TYPE NMKEY
	NMHDR	.hdr 
	ULONG	.nVKey
	ULONG	.uFlags
END TYPE

' Generic structure for a character

TYPE NMCHAR
	NMHDR	.hdr 
	ULONG	.ch
	ULONG	.dwItemPrev 	' item previously selected
	ULONG	.dwItemNext 	' item to be selected
END TYPE

TYPE COMCTL_RECT   ' use RECT
	XLONG	.left 
	XLONG	.top 
	XLONG	.right 
	XLONG	.bottom 
END TYPE

TYPE COMCTL_SYSTEMTIME ' use SYSTEMTIME in kernel32.dec
  USHORT  .year
  USHORT  .month
  USHORT  .weekDay
  USHORT  .day
  USHORT  .hour
  USHORT  .minute
  USHORT  .second
  USHORT  .msec
END TYPE

TYPE COMCTL_SIZE   ' use SIZEAPI
	XLONG	.cx
	XLONG	.cy
END TYPE

TYPE DLLVERSIONINFO
	XLONG	.cbSize
	XLONG	.dwMajor
	XLONG	.dwMinor
	XLONG	.dwBuildNumber
	XLONG	.dwPlatformID
END TYPE

