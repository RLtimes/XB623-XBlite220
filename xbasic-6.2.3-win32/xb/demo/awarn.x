'
' ####################
' #####  PROLOG  #####
' ####################
'
' This program tests that library "warning.dll" exists and works.
'
PROGRAM	"awarn"
VERSION	"0.0002"
'
IMPORT "xst"
IMPORT "xgr"
IMPORT "xui"
IMPORT "warning"
'
DECLARE FUNCTION  Entry ()
DECLARE FUNCTION  InitGui ()
'
'
' ######################
' #####  Entry ()  #####
' ######################
'
FUNCTION  Entry ()
'
	InitGui ()
	DIM text$[10]
	text$[ 0] = "##########################"
	text$[ 1] = "#####  Test Library  #####"
	text$[ 2] = "##########################"
	text$[ 3] = ""
	text$[ 4] = "All this program does is a"
	text$[ 5] = "few function calls to grid"
	text$[ 6] = "function XuiWarning4B() in"
	text$[ 7] = "library \"warning.dll\" - to"
	text$[ 8] = "prove this actually works."
	text$[ 9] = ""
	text$[10] = "Process a MouseUp then die"
	XuiWarning4B (@grid, #CreateWindow, 300, 300, 400, 200, 0, 0)
	XuiSendMessage ( grid, #SetTextArray, 0, 0, 0, 0, 1, @text$[])
	XuiSendMessage ( grid, #SetColor, $$Blue, $$Green, $$Red, $$Yellow, 1, 0)
	XuiSendMessage ( grid, #SetColor, $$BrightCyan, -1, -1, -1, 2, 0)
	XuiSendMessage ( grid, #SetColor, $$BrightGreen, -1, -1, -1, 3, 0)
	XuiSendMessage ( grid, #SetColor, $$LightGreen, -1, -1, -1, 4, 0)
	XuiSendMessage ( grid, #SetColor, $$LightRed, -1, -1, -1, 5, 0)
	XuiSendMessage ( grid, #DisplayWindow, 0, 0, 0, 0, 0, 0)
	XuiSendMessage ( grid, #Redraw, 0, 0, 0, 0, 0, 0)
'
	DO
		XgrPeekMessage (@g, @m, 0, 0, 0, 0, 0, 0)
		XgrProcessMessages (1)
	LOOP UNTIL (m = #WindowMouseUp)
END FUNCTION
